@model InheritanceMapping.Models.MappingModel
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="http://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div>
    @using (Html.BeginForm("ExportExcelProductData", "Home", FormMethod.Get))
    {
        @Html.TextBoxFor(model => model.ProductId)

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" class="btn btn-default" value="ExportExcelProductData" />
            </div>
        </div>
    }

    @using (Html.BeginForm("ExportExcelMappingData", "Home", FormMethod.Get))
    {
        @Html.TextBoxFor(model => model.ProductId)

        @Html.TextBoxFor(model => model.BaseType)

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" class="btn btn-default" value="ExportExcelMappingData" />
            </div>
        </div>
    }
 
</div>